{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    { "source": "/login", "destination": "/login" },
    { "source": "/register", "destination": "/register" },
    { "source": "/videos", "destination": "/videos" },
    { "source": "/admin/:path*", "destination": "/admin/:path*" },
    { "source": "/watch/:path*", "destination": "/watch/:path*" }
  ],
  "github": {
    "silent": true
  }
}
